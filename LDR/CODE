\pard\sa200\sl276\slmult1\f0\fs22\lang9 void setup()\par
\{\par
 \par
  pinMode(2, OUTPUT);\par
  Serial.begin(9600);\par
\}\par
\par
void loop()\par
\{\par
  int x=analogRead(A0);\par
  Serial.println(x);\par
  if (x<500)digitalWrite(2,HIGH);\par
  else digitalWrite(2,LOW);\par
  \par
\}\par
}
